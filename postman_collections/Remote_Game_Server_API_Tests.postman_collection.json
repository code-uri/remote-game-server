								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has promotionId\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('promotionId');",
									"    pm.environment.set('createdPromotionId', jsonData.promotionId);",
									"});",
									"",
									"pm.test(\"Response has promotionRefId\", function () {",
